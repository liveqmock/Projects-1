			<operator operatorId="User_Approve" operatorName="企业用户开通审核"
				urlmapping="/hps/userMgr">
			</operator>
		</modelName>
		
		<modelName modleName="角色管理">
			<operator operatorId="Frame_Role_Manager" operatorName="查询角色"
				urlmapping="/sys/roleMgr,/sys/roleMgr/viewRoleFormPage">
