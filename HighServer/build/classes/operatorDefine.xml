			<operator operatorId="Frame_User_Edit" operatorName="编辑用户"
				urlmapping="/sys/userMgr/editUserFormPage">
			</operator>
			<operator operatorId="Frame_User_Delete" operatorName="删除用户"
				urlmapping="/sys/userMgr/deleteUser">
