			<operator operatorId="Frame_Role_Permission" operatorName="分配权限"
				urlmapping="/sys/roleMgr/authorizePage">
			</operator>
		</modelName>

		<modelName modleName="日志管理">
			<operator operatorId="Frame_LogManager" operatorName="日志管理">
