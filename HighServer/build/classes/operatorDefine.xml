			<operator operatorId="ScoreProduct_Edit" operatorName="编辑商品"
				urlmapping="/product/edit_form">
			</operator>
			<operator operatorId="ScoreProduct_Delete" operatorName="删除商品"
				urlmapping="/product/delete">
			</operator>
