			<operator operatorId="CMS_Mgr_Delete"
				operatorName="删除文章" urlmapping="/cmscontent/main">
			</operator>
		</modelName>

		<modelName modleName="积分商城">
			<operator operatorId="Score_Product" operatorName="查看积分商品"
