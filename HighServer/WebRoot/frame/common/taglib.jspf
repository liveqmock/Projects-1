<%@ page language="java" contentType="text/html; charset=UTF-8"
	pageEncoding="UTF-8"%>
<%@ taglib prefix="c" uri="http://java.sun.com/jsp/jstl/core"%>
<%@ taglib prefix="fn" uri="http://java.sun.com/jsp/jstl/functions"%>
<%@ taglib prefix="fmt" uri="http://java.sun.com/jsp/jstl/fmt"%>
<%@ taglib tagdir="/WEB-INF/tags" prefix="nui"%>
<%@ taglib uri="http://www.newsoft.com/tags-operator" prefix="operator"%>
<%@ taglib uri="http://www.newsoft.com/tags-operator"
	prefix="operation"%>
<c:set var="ctx" scope="session"
	value="${pageContext.request.contextPath}" />
<c:set var="servurl" scope="session"
	value="http://${pageContext.request.serverName}:${pageContext.request.serverPort}${pageContext.request.contextPath}" />

